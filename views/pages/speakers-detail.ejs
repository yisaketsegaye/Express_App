
  <div class="container mt-4">
    <div class="row">
      <div class="col-md-8">
        <article class="speakerslist maincontent">
          <h1 class="speakerslist-title"><%= speaker.title %></h1>
          <div class="speakerslist-name">
            with
            <a href="./<%= speaker.shortname %>"> <%= speaker.name %>Post</a>
          </div>
          <p class="speakerslist-info mt-2">
            <a href="./<%= speaker.shortname %>.html"> </a>
            <img
              class="speakerslist-img img-fluid"
              src="/images/speakers/<%=speaker.shortname%>.jpg"
              alt="Photo of Hilary Goldywynn Post"
            />
          </p>
          <%- speaker.description %>
          <p></p>
        </article>
      </div>
      <aside class="col-md-4">
      
        <%- include('./partials/side-artwork' , {speakerArtwork: speakerArtwork} ) %>
      </aside>
    </div>
  </div>
</body>
